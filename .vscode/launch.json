            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/qc2srgb",
            "args": ["--verbose",
                "--display","video",
                "--video-path","/home/muma/Working/Projects/Code/quadcast2Srgb/badapp/stars.bin",
                "--video-format","rgb",
                "--video-framerate","1",
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
